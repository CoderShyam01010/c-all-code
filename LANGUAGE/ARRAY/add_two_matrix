#include <stdio.h>
int main()
{
   
    int m,a[m][m], b[m][m],  i,j;
    printf("enter array: ");
    scanf("%d", &m);
    for (i = 0; i < m; i++)
    {
        for (int j = 0; j < m; j++)
        {
            scanf("%d ", &a[i][j]);
        }
    }
    for (i = 0; i < m; i++)
    {
        for (int j = 0; j < m; j++)
        {
            scanf("%d ", &b[i][j]);
        }
    }
    for (i = 0; i < m; i++)
    {
        for (int j = 0; j < m; j++)
        {
            printf("%d ", b[i][j] + a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
